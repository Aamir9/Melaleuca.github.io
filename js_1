Type.registerNamespace('Melaleuca.Web');
Melaleuca.Web.AjaxManager=function() {
Melaleuca.Web.AjaxManager.initializeBase(this);
this._timeout = 0;
this._userContext = null;
this._succeeded = null;
this._failed = null;
}
